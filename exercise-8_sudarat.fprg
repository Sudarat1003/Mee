                                            <assign variable="add" expression="How*price"/>
                                            <output expression="&quot;Total : &quot; &amp; add &amp; &quot; THB &quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="ch==4">
                                                <then>
                                                    <declare name="price" type="Integer" array="False" size=""/>
                                                    <assign variable="price" expression="55"/>
                                                    <output expression="&quot;Your order : Beer 55 THB&quot;" newline="True"/>
                                                    <output expression="&quot;How many do you need :&quot;" newline="True"/>

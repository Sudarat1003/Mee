                                                            <declare name="add" type="Integer" array="False" size=""/>
                                                            <input variable="How"/>
                                                            <assign variable="add" expression="How*price"/>
                                                            <output expression="&quot;Total : &quot; &amp; add &amp; &quot; THB &quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <output expression="&quot;You not order&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>

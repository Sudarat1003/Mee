                    <output expression="&quot;Welcome to Drink Shop&quot;" newline="True"/>
                    <output expression="&quot;-----------Menu-----------&quot;" newline="True"/>
                    <output expression="&quot;1. Coca Cola 15 THB&quot;" newline="True"/>
